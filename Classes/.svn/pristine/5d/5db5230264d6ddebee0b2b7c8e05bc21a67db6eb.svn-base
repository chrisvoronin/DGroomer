#import <Foundation/Foundation.h>
#import <sqlite3.h>

@interface TransactionDetail : NSObject {
	NSInteger	transactionDetailID;
	NSInteger	appointmentID;
	NSInteger	clientID;
	NSInteger	productID;
	NSInteger	serviceID;
	NSInteger	transactionID;
	NSNumber	*preTaxAmount;
	NSNumber	*discountedAmount;
	NSNumber	*taxedAmount;
	NSInteger	giftCertificateID;
	NSInteger	transactionNumber;
}

@property (assign, nonatomic) NSInteger transactionDetailID;
@property (assign, nonatomic) NSInteger appointmentID;
@property (assign, nonatomic) NSInteger clientID;
@property (assign, nonatomic) NSInteger productID;
@property (assign, nonatomic) NSInteger serviceID;
@property (assign, nonatomic) NSInteger transactionID;
@property (assign, nonatomic) NSNumber *preTaxAmount;
@property (assign, nonatomic) NSNumber *discountedAmount;
@property (assign, nonatomic) NSNumber *taxedAmount;
@property (assign, nonatomic) NSInteger giftCertificateID;
@property (assign, nonatomic) NSInteger transactionNumber;

// Finalize (delete) all of the SQLite compiled queries.
+ (void)finalizeStatements;

- (id)initWithTransactionDetailData:(NSInteger)transDetailID apptID:(NSInteger)apptID cID:(NSInteger)cID prodID:(NSInteger)prodID servID:(NSInteger)sID transID:(NSInteger)transID preTax:(NSNumber*)preTax discAm:(NSNumber*)discAm taxAmount:(NSNumber*)taxA giftCertID:(NSInteger)gCertID transNum:(NSInteger)transNum;

@end
