#import <Foundation/Foundation.h>
#import <sqlite3.h>

@interface TransactionPayment : NSObject {
	NSInteger transactionPaymentID;
	NSInteger transactionID;
	NSString *transactionType;
	NSNumber *transPriceAmount;
	NSNumber *transDiscAmount;
	NSNumber *transTaxedAmount;
	NSNumber *transTaxPaid;
	NSNumber *transTipAmount;
	NSNumber *transAmountPaid;
	NSNumber *transChange;
	NSNumber *transSubtotal;
	NSInteger giftCertificateID;
	NSInteger transCheckNumber;
}

@property (assign, nonatomic) NSInteger transactionPaymentID;
@property (assign, nonatomic) NSInteger transactionID;
@property (assign, nonatomic) NSString *transactionType;
@property (assign, nonatomic) NSNumber *transPriceAmount;
@property (assign, nonatomic) NSNumber *transDiscAmount;
@property (assign, nonatomic) NSNumber *transTaxedAmount;
@property (assign, nonatomic) NSNumber *transTaxPaid;
@property (assign, nonatomic) NSNumber *transTipAmount;
@property (assign, nonatomic) NSNumber *transAmountPaid;
@property (assign, nonatomic) NSNumber *transChange;
@property (assign, nonatomic) NSNumber *transSubtotal;
@property (assign, nonatomic) NSInteger giftCertificateID;
@property (assign, nonatomic) NSInteger transCheckNumber;

// Finalize (delete) all of the SQLite compiled queries.
+ (void)finalizeStatements;

- (id)initWithTransPaymentData:(NSInteger)transPID transIDL:(NSInteger)transID transType:(NSString*)transType tpa:(NSNumber*)tpa tda:(NSNumber*)tda tta:(NSNumber*)tta ttp:(NSNumber*)ttp ttam:(NSNumber*)ttam tap:(NSNumber*)tap tc:(NSNumber*)tc ts:(NSNumber*)ts giftCertID:(NSInteger)giftCertID tcn:(NSInteger)tcn;

@end
